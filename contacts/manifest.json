{
"manifest_version": 2,
   "app": {
      "launch": {
         "container": "tab",
         "web_url": "https://www.google.com/contacts"
      },
      "urls": [ "https://www.google.com/contacts" ]
   },
   "description": "Google Contacts",
   "icons": {
      "128": "contacts.png"
   },
   "name": "Google Contacts",
   "offline_enabled": true,
   "permissions": [ "unlimitedStorage", "notifications" ],
   "version": "0.0.1"
}
